{
    "name": "kodewbit/librivox",
    "description": "A small library to obtain data from the books hosted on LibriVox.",
    "keywords": ["librivox", "api"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Leonardo Yanes Batista",
            "email": "leo95batista@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^6.5.5|^7.0.1",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Kodewbit\\LibriVox\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kodewbit\\LibriVox\\LibriVoxServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
